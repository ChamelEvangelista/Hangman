{
  "expo": {
    "name": "Hangman Game",
    "slug": "hangman-game-evangelista",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "scheme": "hangmancha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.evangelistacha.hangman"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#0f172a"
      },
      "package": "com.evangelistacha.hangman",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "single",
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "2421d636-e884-4b7a-8c88-faa0ac9c75e5"
      }
    }
  }
}
